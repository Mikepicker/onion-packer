{
  "name": "OnionPacker",
  "version": "1.0.0",
  "description": "Texture & Images Organizer",
  "main": "main.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel src/ --watch --out-dir build/",
    "start": "electron .",
    "dev": "npm run babel | npm start",
    "bundle_win": "electron-packager . onion-packer --overwrite --icon=./icon.ico --platform=win32",
    "bundle_osx": "electron-packager . onion-packer --overwrite --icon=./icon.icns --platform=darwin"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "electron": "^1.4.15"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "chokidar": "^1.6.1",
    "electron": "^1.6.2",
    "fs": "0.0.1-security",
    "fs-extra": "^2.0.0",
    "lodash": "^4.17.4",
    "ncp": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-three-renderer": "^3.0.2",
    "rimraf": "^2.5.4",
    "three": "^0.82.1"
  }
